<h2 id="create-tlt">Create a new drone</h2>
<form action="/create-form" method="post">
    <label for="name">Name</label>
    <input type="text" name="name" id="name"/>
    
    <label for="propellers">Propellers</label>
    <input type="number" name="propellers" id="propellers"/>

    <label for="maxSpeed">maxSpeed</label>
    <input type="number" name="maxSpeed" id="maxSpeed"/>


    <button type="submit">Add Drone</button>
</form>